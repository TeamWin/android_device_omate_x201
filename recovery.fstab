# mount point	fstype		device			[device2]

/boot		emmc		/dev/bootimg
/cache		ext4		/dev/block/mmcblk0p5
/data		ext4		/dev/block/mmcblk0p6
/system         ext4            /dev/block/mmcblk0p4
#/misc		emmc		misc
/recovery	emmc		/dev/recovery
/internal_sd	vfat		/dev/block/mmcblk0p7				flags=display="Internal Storage";settingsstorage;wipeingui
/external_sd	vfat		/dev/block/mmcblk1p1	/dev/block/mmcblk1	flags=display="Micro SD";storage;wipeingui;removable
